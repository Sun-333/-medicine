<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 1 配置类和表对应 
		class标签
		name属性：实体类全路径
		table属性：数据库表名称
	-->
	<class name="cn.cqupt.entity.UserToken" table="usertoken">
		<id name="accountId" column="AccountId">
			<generator class="assigned"></generator>
		</id>
		<property name="accountName" column="AccountName"></property>
		<property name="accountPwd" column="AccountPwd"></property>
		<property name="userName" column="UserName"></property>
		<property name="userId" column="UserId"></property>
		<property name="roleId" column="roleId"></property>
		<property name="roleName" column="roleName"></property>
		<property name="rolePerm" column="rolePerm"></property>
		<property name="depId" column="depId"></property>
		<property name="depName" column="depName"></property>
		<property name="userTel" column="UserTel"></property>
		<property name="userEmail" column="UserEmail"></property>
		<property name="validTime" column="ValidTime"></property>
	</class>
</hibernate-mapping>





